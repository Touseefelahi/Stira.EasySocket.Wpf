<Window x:Class="Stira.EasySocket.Sample.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:socket="clr-namespace:Stira.EasySocket.Views;assembly=Stira.EasySocket"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525">
    <Grid>
        <socket:Control IsDeveloperControlVisible="True" DataContext="{Binding SocketControl}" />
    </Grid>
</Window>